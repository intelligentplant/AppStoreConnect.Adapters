{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2AndHttp3"
    }
  },
  "AppStoreConnect": {
    "Adapter": {
      "Settings": {
        "Name": "Wave Generator",
        "Description": "Generates sinusoid, sawtooth, square and triangle waves.",
        "SampleInterval": "00:00:05"
      }
    }
  },
  "OpenTelemetry": {
    "Exporters": {
      "OTLP": {
        "Enabled": true
      }
    }
  }
}
